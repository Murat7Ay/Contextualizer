{
  "id": "corp-lookup-basic",
  "name": "Corp Lookup Basic",
  "version": "1.0.0",
  "author": "Murat",
  "description": "Kurumsal müşteri bilgilerini arar",
  "tags": [
    "lookup",
    "corporate",
    "customer"
  ],
  "dependencies": [
    "show_window",
    "copytoclipboard"
  ],
  "handlerJson": {
    "name": "Corp Lookup Basic",
    "type": "lookup",
    "screen_id": "markdown2",
    "title": "Corp Basic",
    "path": "D:\\Contextualizer\\pre-defined\\corporates.txt",
    "delimiter": "|~#~|",
    "key_names": [
      "kurumoid",
      "kodu"
    ],
    "value_names": [
      "kurumoid",
      "unvankodu",
      "kodu",
      "tahsilatturuadi"
    ],
    "actions": [
      {
        "name": "show_window",
        "key": "_formatted_output"
      },
      {
        "name": "copytoclipboard",
        "key": "kurumoid",
        "conditions": {
          "operator": "and",
          "conditions": [
            {
              "field": "_selector_key",
              "operator": "equals",
              "value": "kodu"
            }
          ]
        }
      },
      {
        "name": "copytoclipboard",
        "key": "kodu",
        "conditions": {
          "operator": "and",
          "conditions": [
            {
              "field": "_selector_key",
              "operator": "equals",
              "value": "kurumoid"
            }
          ]
        }
      }
    ],
    "output_format": "$file:D:\\Contextualizer\\markdown_templates\\corp_template.md"
  },
  "metadata": {
    "category": "Lookup",
    "handlerType": "lookup"
  }
}

